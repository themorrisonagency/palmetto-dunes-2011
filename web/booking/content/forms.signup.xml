		<formcontent>
			<form class="standard validate" id="signup">
				<fieldset>
					<legend>Contact Information</legend>
					<div class="field">
						<label for="prefix">Prefix</label>
						<input type="text" class="textfield" value="" name="prefix" id="prefix" placeholder="Prefix" />
					</div>
					<div class="field">
						<label for="first_name">First Name:</label>
						<input type="text" name="first_name" id="first_name" maxlength="100" class="textfield required" value="" placeholder="First Name" />
					</div>
					<div class="field">
						<label for="last_name">Last Name:</label>
						<input type="text" name="last_name" id="last_name" maxlength="100" class="textfield required" value="" placeholder="Last Name" />
					</div>
					<div class="field">
						<label for="email">Email Address:</label>
						<input type="text" name="email" id="email" maxlength="100" class="large email textfield required" value="" placeholder="Email Address" />
					</div>
					<div class="field">
						<label for="zip">Postal Code:</label>
						<input type="text" name="zip" id="zip" maxlength="10" class="small textfield" value="" placeholder="Postal Code" />
					</div>
				</fieldset>
				<div class="buttons">
					<input type="submit" name="submit" id="submit" class="submit" value="Submit" />
				</div>
			</form>		
		</formcontent>
